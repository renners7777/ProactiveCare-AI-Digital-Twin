{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "mobile",
  "version": "1.0.0",
  "private": true,
  "description": "Mobile application for ProactiveCare AI Digital Twin",
  "author": "Chris Renshaw",
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "EXPO_DEBUG=true expo start --clear",
    "android": "EXPO_DEBUG=true expo start --android --clear",
    "ios": "EXPO_DEBUG=true expo start --ios --clear",
    "web": "expo start --web",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "clean": "rm -rf node_modules .expo",
    "postinstall": "expo-yarn-workspaces postinstall"
  },
  "dependencies": {
    "@kingstinct/react-native-healthkit": "^1.0.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@supabase/supabase-js": "^2.39.7",
    "common": "*",
    "expo": "~53.0.0",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.0.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo-yarn-workspaces": "^1.5.2",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.7",
    "typescript": "^5.1.3"
  }
}
