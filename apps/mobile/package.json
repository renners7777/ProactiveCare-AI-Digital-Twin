{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "mobile",
  "version": "1.0.0",
  "private": true,
  "description": "Mobile application for ProactiveCare AI Digital Twin",
  "author": "Chris Renshaw",
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --clear",
    "android": "expo start --android --clear",
    "ios": "expo start --ios --clear",
    "web": "expo start --web",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "clean": "rm -rf node_modules .expo",
    "postinstall": "expo-yarn-workspaces postinstall"
  },
  "dependencies": {
    "@kingstinct/react-native-healthkit": "^1.0.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@supabase/supabase-js": "^2.39.7",
    "common": "*",
    "expo": "~50.0.14",
    "expo-application": "~5.8.4",
    "expo-background-fetch": "~11.8.1",
    "expo-health-connect": "0.1.1",
    "expo-sensors": "~12.9.1",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.13",
    "lucide-react-native": "^0.344.0",
    "nativewind": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-chart-kit": "^6.12.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.0.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.2.45",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo-yarn-workspaces": "^1.5.2",
    "jest": "^29.7.0",
    "jest-expo": "~50.0.0",
    "typescript": "^5.1.3"
  }
}
